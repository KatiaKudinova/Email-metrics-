SELECT year, month,
       sum(revenue) as revenue,
       sum(cost) as cost
FROM (
SELECT extract(year from c.date) as year,
       extract(month from c.date) as month,
       sum(b.price) as revenue,
       CAST(NULL AS NUMERIC) AS cost
      FROM `DA.order` a
join `DA.product` b  
on a.item_id = b.item_id
join `DA.session` c
on a.ga_session_id = c.ga_session_id
GROUP BY 1, 2
UNION ALL
       SELECT
       extract(year from date) as year,
       extract(month from date) as month,
       CAST(NULL AS NUMERIC) AS revenue,
       sum(cost) as cost
      FROM `DA.paid_search_cost`
GROUP BY 1, 2) as union_all
GROUP BY 1,2;
